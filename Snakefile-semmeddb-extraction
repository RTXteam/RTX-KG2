rule SemMedDB:
    input:
        config['BUILD_DIR'] + "/validation-placeholder.empty"
    output:
        config['SEMMED_TUPLELIST_FILE']
    log:
        config['BUILD_DIR'] + "/extract-semmeddb.log"
    shell:
        "bash -x " + config['CODE_DIR'] + "/extract-semmeddb.sh {output} " + config['TEST_FLAG'] + " > {log} 2>&1"

